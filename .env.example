# DocumentIulia.ro Environment Variables
# Copy this file to .env and fill in the values

# Database
POSTGRES_USER=documentiulia
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=documentiulia
DATABASE_URL=postgresql://documentiulia:your_secure_password_here@localhost:5432/documentiulia?schema=public

# Security
JWT_SECRET=your_jwt_secret_at_least_32_chars_long

# AI Integration
GROK_API_KEY=your_grok_api_key

# ANAF Integration
ANAF_API_URL=https://api.anaf.ro

# Frontend
NEXT_PUBLIC_API_URL=https://documentiulia.ro/api
BACKEND_URL=http://localhost:3001

# Optional
NODE_ENV=production
